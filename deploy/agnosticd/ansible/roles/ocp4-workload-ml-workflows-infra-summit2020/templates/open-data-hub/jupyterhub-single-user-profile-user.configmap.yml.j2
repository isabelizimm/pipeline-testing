apiVersion: v1
data:
  profile: |
    env:
      JUPYTER_PRELOAD_REPOS: "{{ workshop_preload_repos }}"
      JUPYTER_ENABLE_LAB: '1'
    gpu: '0'
    last_selected_image: ml-workflows-notebook:latest
    last_selected_size: None
kind: ConfigMap
metadata:
  labels:
    app: jupyterhub
  name: jupyterhub-singleuser-profile-{{ suffix }}
  namespace: {{ namespace }}
